<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity Entidade.
    -->
    <changeSet id="20220919230156-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="process_id"
                                 baseTableName="entidade"
                                 constraintName="fk_entidade__process_id"
                                 referencedColumnNames="id"
                                 referencedTableName="process"/>

        <addForeignKeyConstraint baseColumnNames="application_id"
                                 baseTableName="entidade"
                                 constraintName="fk_entidade__application_id"
                                 referencedColumnNames="id"
                                 referencedTableName="application"/>
    </changeSet>
</databaseChangeLog>
